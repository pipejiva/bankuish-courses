{
  "name": "bankuish-courses",
  "version": "0.0.1",
  "description": "Felipe Jimenez code-challenge",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t bankuish-courses .",
    "docker:run": "docker run -p 3000:3000 -d bankuish-courses",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Felipe Jimenez <pipe2211@gotmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.0.0",
    "@loopback/authentication-jwt": "^0.13.0",
    "@loopback/boot": "^6.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/repository": "^6.0.0",
    "@loopback/rest": "^13.0.0",
    "@loopback/rest-explorer": "^6.0.0",
    "@loopback/service-proxy": "^6.0.0",
    "@types/request": "^2.48.8",
    "@types/request-promise": "^4.1.48",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.8.0",
    "loopback-connector-mysql": "^6.2.0",
    "request-promise": "^4.2.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@loopback/build": "^10.0.0",
    "@loopback/eslint-config": "^14.0.0",
    "@loopback/testlab": "^6.0.0",
    "@types/node": "^14.18.47",
    "eslint": "^8.40.0",
    "mocha": "^10.2.0",
    "sinon": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.5"
  }
}
